		],
		"nodes": [
			"dist/nodes/OpenCti/OpenCti.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/node": "^22.0.0",
		"eslint": "9.32.0",
		"n8n-workflow": "^2.7.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
