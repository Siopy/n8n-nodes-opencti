{
	"name": "n8n-nodes-opencti",
	"version": "0.1.1",
	"description": "n8n community node for OpenCTI - Cyber Threat Intelligence Platform",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"opencti",
		"cyber-threat-intelligence",
		"stix",
		"graphql"
	],
	"license": "MIT",
	"author": {
		"name": "Siopy",
		"email": "contact@siopysec.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Siopy/n8n-nodes-opencti"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"n8n": {
